CC=g++
CFLAGS=-Wall -std=c++11

TARGET = NTRIPClient

all: $(TARGET)
	mv $(TARGET) ../run


SRCS = ntrip_client.cpp \
	../util/ntrip_util.cpp 


OBJS = $(SRCS:.cpp=.o)
$(TARGET):$(OBJS)
#	@echo TARGET:$@
#	@echo OBJECTS:$^
	$(CC) -o $@ $^

 
INC = -I../include

clean:
	rm -rf ../run/* $(OBJS)

%.o:%.cpp
	$(CC) $(CFLAGS) $(INC) -o $@ -c $<
